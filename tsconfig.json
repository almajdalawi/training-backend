{
    "compilerOptions": {
        "target": "ES2020",
        "lib": [
            "dom",
            "dom.iterable",
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "ES2020",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "baseUrl": "./",
        "typeRoots": [
            "node_modules/@types",
            "types"
        ]
    }
}